#!/bin/sh
#

dims="x:1-1024 y:1-2024"


EVENTS="
x	y
-	-
512	511
512	512
512	513
512	514
512	515
511	512
512	512
513	512
514	512
515	512
511	511
510	510
509	509
508	508
507	507"

echo "$EVENTS" | tabletofits $dims | funimage STDIN[1][*,1] STDOUT > cross1.fits
echo "$EVENTS" | tabletofits $dims | funimage STDIN[1][*,2] STDOUT > cross2.fits
echo "$EVENTS" | tabletofits $dims | funimage STDIN[1][*,4] STDOUT > cross4.fits


