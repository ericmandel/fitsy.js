int itlp2i (double dp, double tlmin, double binsiz, int type)
{
  if ( (binsiz == 1.0) || (binsiz <= 0.0) ) {
    return (int)((dp - tlmin) + 1.0);
  }  else {
    return (int)((dp - tlmin)/binsiz + 1.0);
  }
}

value = itlp2i((double)eptr[offset[j]], minval[j], binsiz[j], type);

